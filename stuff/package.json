{
  "name": "stuff",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "express": "^4",
    "@sap/xssec": "^4",
    "@cap-js/hana": "^1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project3": "cds watch --open project3/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy stuff --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "sapux": [
    "app/admin-books",
    "app/browse",
    "app/project3"
  ],
  "cds": {
    "server": {
      "index": true
    },
    "requires": {
      "auth": "xsuaa"
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
